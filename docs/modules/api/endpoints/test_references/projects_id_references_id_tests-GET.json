{
    "endpoint": "/projects/:project_id/references/:reference_id/tests",
    "method": "GET",
    "attributes" : {
        ":project_id" : {
            "type": "string/integer",
            "required": true,
            "description": "The id or slug of the project to get the test references of."
        },
        ":reference_id" : {
            "type": "integer",
            "required": true,
            "description": "The id of the reference set to get the tests references of."
        }
    },
    "examples" : [
        {
            "arguments" : {
                "project_id" : 1,
                "reference_id": 1
            }
        }
    ]
}
